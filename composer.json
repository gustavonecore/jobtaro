{
	"name": "gustavonecore/jobtaro",
	"description": "PHP Job Server",
	"type": "library",
	"license": "MIT",
	"authors": [
		{ "name": "Gustavo Delgado R.", "email":"gustavo@onecore.cl" }
	],
	"require": {
		"php": ">=7.2",
		"php-amqplib/php-amqplib": "^2.9",
		"enqueue/redis": "^0.9.7",
		"symfony/process": "^4.3",
		"psr/container": "^1.0",
		"predis/predis": "^1.1",
		"psr/log": "^1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.0",
		"phpmd/phpmd" : "1.5.*",
		"phploc/phploc": "*",
		"sebastian/phpcpd": "*"
	},
	"autoload": {
		"psr-4": {
			"Gcore\\": "src/",
			"Test\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"minimum-stability": "dev",
    "prefer-stable": true
}
